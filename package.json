{
  "name": "@BazaarDog/bazaar-dog-stencil",
  "private": true,
  "version": "0.0.2",
  "description": "Bazaar Dog",
  "files": [
    "dist"
  ],
  "scripts": {
    "buildNoPR": "stencil build  --stats",
    "build": "stencil build  --prerender --stats",
    "dev": "sd concurrent \"stencil build --dev --stats --watch\" \"stencil-dev-server\" ",
    "devWithSW": "sd concurrent \"stencil build --dev --service-worker --watch\" \"stencil-dev-server\" ",
    "functionDev": "stencil build --dev --watch",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "deploy": "npm run build"
  },
  "dependencies": {
    "@ionic/core": "0.2.2",
    "@stencil/core": "^0.7.24",
    "@stencil/router": "0.1.3",
    "am-rating": "0.0.7",
    "baguettebox.js": "^1.10.0",
    "bazaar-dog-offline": "git://github.com/BazaarDog/bazaar-dog-offline#1b699da839bf146ad1864504272974426428eaa8",
    "greenlet": "^0.1.2",
    "intersection-observer": "~0.5.0",
    "localforage": "1.7.0",
    "localforage-cordovasqlitedriver": "^1.7.0",
    "purify": "^3.3.1",
    "st-img": "^0.3.0"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/sass": "0.0.2",
    "@stencil/utils": "latest",
    "webpack": "^4.6.0"
  },
  "repository": {
    "type": "git",
